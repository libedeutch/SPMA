\name{true_positive}
\alias{true_positive}
\title{true_positive}
\description{
Calculate true positive rate from confusion matrix
}
\usage{
true_positive(confuse)
}
\arguments{
\item{confuse}{a confuse matrix for two classes.}
}
\details{
\tabular{cccc}{
 \tab \tab  True \cr
 \tab \tab P \tab N \cr
 Prediction \tab P \tab TP \tab FP\cr
 \tab N  \tab FN \tab TN\cr
}
\deqn{\text{TPR} = \frac{TP}{TP+FN}}
}


