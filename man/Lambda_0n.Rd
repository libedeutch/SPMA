\name{Lambda_0n}
\alias{Lambda_0n}
\title{Lambda_0n}
\description{
Approximate baseline hazard function for Cox PH model using Berstein Polynomial
}
\usage{
Lambda_0n(t,phi,m,u,v)
}
\arguments{
\item{t}{a vector of event time.}
\item{phi}{a term bounded by a positive number M.}
\item{u}{upper bound of the observation time.}
\item{v}{lower bound of the observation time.}
\item{m}{degree of freedom of the Berstein polynomials.}

}
\details{
\deqn{\Lambda_{0n}(t) = \sum_{k=0}^m\phi_k^*B_k(t,m,u,v)}
\deqn{\phi_k^* = \sum_{j=1}^k\exp(\phi_i)}
printResult(rre, intervalCensor = T)
}
